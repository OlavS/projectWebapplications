@model List<Model.ViewModels.ErrorLogVM>

@{
    ViewBag.Title = "ErrorLog";
}

@section viewTitle{
    <h2>ErrorLog</h2>
}

@section SearchField{
    <div id="sidebarSearch">
        <div class="searchTitle">
            <p>Søkefelt:</p>
        </div>
        @using (Html.BeginForm("SearchErrorLog", "Admin", FormMethod.Post))
        {
            @Html.TextBox("searchString", "", new
            {
                @class = "form-control",
                @placeholder = "Search i log",
                @type = "search",
                @id = "searchField",
                name = "searchField"
            })
        }
    </div>
}

<div class="container adminContainer adminTableContainer">
    <table id="ChangeLogTable" class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model[0].Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].Time)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].Message)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].Parameter)
            </th>
            <th>
                @Html.DisplayNameFor(model => model[0].StackTrace)
            </th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Time)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Message)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Parameter)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.StackTrace)
                </td>
            </tr>
        }
    </table>
</div>